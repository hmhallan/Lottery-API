node {
    stage('git clone'){
        git branch: 'azeitado', url: 'https://github.com/hmhallan/Lottery-API.git'

    }
    stage('Build'){
        parallel(
	   	"Analise de c√≥digo":{
			withCredentials([string(credentialsId: 'SONAR_TOKEN', variable: 'SENHA_SONAR')]) {
		    		sh "./gradlew -DSonar.login=${SENHA_SONAR} sonarqube"
			}
		},
		"Build":{
			sh "./gradlew build"
		}
        )
        
    }
    
}
